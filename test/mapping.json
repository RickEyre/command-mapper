{
 "ls": {
    "shortcut": "l",
    "default": "-ls",
    "options": {
      "t": "-lkh"
    }
  },
  "git": {
    "shortcut": "g",
    "default": "help",
    "mappings": {
      "commit": {
        "shortcut": "c",
        "default": "-a",
        "options": {
          "m": "-am"
        }
      },
      "diff": {
        "shortcut": "d",
        "default": "HEAD",
        "always": "--color",
        "options": {
          "h": "--histogram"
        }
      }
    }
  }
}
